# Compile an scss file

> set SassKeys.useDartSass := false

> assets

$ exists target/web/public/main/stylesheets/deeply/nested/asset/directory/basic.css target/web/public/main/stylesheets/deeply/nested/asset/directory/basic.css.map

> clean

> set SassKeys.useDartSass := true

> assets

$ exists target/web/public/main/stylesheets/deeply/nested/asset/directory/basic.css target/web/public/main/stylesheets/deeply/nested/asset/directory/basic.css.map